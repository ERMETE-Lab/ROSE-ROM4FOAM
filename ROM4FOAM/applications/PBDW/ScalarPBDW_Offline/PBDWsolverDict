Offline_parameters
{
    field      T;
    BasisNumber   50;       // maximum size of the reduced basis
    foldersList ("Folder_1" "Folder_2") ;
    
    // The following are specific for the GEIM;
    MaxSensorsNumber 20;    
    SensorsVariance 0.0001;
